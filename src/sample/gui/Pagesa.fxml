<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<GridPane styleClass="gp" hgap="10" vgap="10" alignment="CENTER" xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml">

    <padding>
        <Insets left="10" right="10" bottom="15" top="10"/>
    </padding>

    <Label text="%pagesa" GridPane.rowIndex="0" GridPane.columnIndex="0" styleClass="bigFont"/>
    <TextField fx:id="txtPagesa" GridPane.rowIndex="1" GridPane.columnIndex="0" style="-fx-font-size: 22"/>
    <Button onAction="#shtoPagesen" text="%ok" GridPane.rowIndex="2" GridPane.columnIndex="0" GridPane.columnSpan="2" minWidth="150"/>

</GridPane>
